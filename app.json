{
  "expo": {
    "name": "RN-project-01",
    "slug": "RN-project-01",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "permissions": {
      "camera": {
        "allowsRecording": true,
        "allowsExternalAttachments": true,
        "allowsEditing": false
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "hermes",
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "softwareKeyboardLayoutMode": "pan"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
